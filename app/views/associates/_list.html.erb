
<table class="table table-striped table-bordered table-condensed">
  <caption Associate</caption>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Date Of Joinin (y-m-d)</th>
      <th>Issued Items**</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>

<% if @associates!=nil %>
  <% @associates.each do |associate| %>
    <tr>
      <td><%= associate.name%></td>
      <td><%= associate.email %></td>
      <td><%= associate.dateOfJoining %></td>
      <td><%= link_to "Issued Items", '#' %></th>
      <td><%=link_to "edit", edit_associate_path(associate[:id]) %></td>
      <td><%= link_to "delete ", associate, method: :delete, data: { confirm: "You sure?" } %></td>
    </tr>     
  <% end %>
<% end %>

  </thead>
</table>

